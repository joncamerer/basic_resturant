<%= form_for @account, url: member_account_path do |f| %>
  <div class="form-group">
    <%= f.label :visit_count %>
    <%= f.text_field :visit_count, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :loyalty_points %>
    <%= f.text_field :loyalty_points, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :card_number %>
    <%= f.text_field :card_number, class: 'form-control' %>
  </div> 
  <div class="form-group">
    <%= f.label :join_date %>
    <%= f.date_field :join_date, class: 'form-control' %>
  </div> 
  <div class="form-group">
    <%= f.label :last_visit %>
    <%= f.date_field :last_visit, class: 'form-control' %>
  </div> 
  <div>
    <%= f.submit "Submit", class: 'btn btn-primary' %>
  </div>
<% end %>
  
